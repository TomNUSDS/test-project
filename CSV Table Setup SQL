CREATE TABLE Inventory (
	InventoryID VARCHAR(32) PRIMARY KEY NOT NULL,
	ProjectID VARCHAR(32) NOT NULL,
	CoCCode VARCHAR(6) NOT NULL,
	HouseholdType INT NOT NULL,
	Availability INT,
	UnitInventory INT NOT NULL,
	BedInventory INT NOT NULL,
	CHVetBedInventory INT,
	YouthVetBedInventory INT,
	VetBedInventory INT,
	CHYouthBedInventory INT,
	YouthBedInventory INT,
	CHBedInventory INT,
	OtherBedInventory INT,
	ESBedType INT,
	InventoryStartDate DATE NOT NULL,
	InventoryEndDate DATE,
	DateCreated DATETIME NOT NULL,
	DateUpdated DATETIME NOT NULL,
	UserID VARCHAR(32) NOT NULL,
	DateDeleted DATETIME,
	ExportID VARCHAR(32) NOT NULL
	);

CREATE TABLE Organization (
	OrganizationID VARCHAR(32) PRIMARY KEY,
	OrganizationName VARCHAR(200),
	VictimServiceProvider INT,
	OrganizationCommonName VARCHAR(200),
	DateCreated DATETIME,
	DateUpdated DATETIME,
	UserID VARCHAR(32),
	DateDeleted DATETIME,
	ExportID VARCHAR(32)
	);

CREATE TABLE Project (
	ProjectID VARCHAR(32) PRIMARY KEY NOT NULL,
	OrganizationID VARCHAR(32) NOT NULL,
	ProjectName VARCHAR(200) NOT NULL,
	ProjectCommonName VARCHAR(200),
	OperatingStartDate DATE NOT NULL,
	OperatingEndDate DATE,
	ContinuumProject INT NOT NULL,
	ProjectType INT,
	HousingType INT,
	RRHSubType INT,
	ResidentialAffiliation INT,
	TargetPopulation INT,
	HOPWAMedAssistedLivingFac INT,
	PITCount INT,
	DateCreated DATETIME NOT NULL,
	DateUpdated DATETIME NOT NULL,
	UserID VARCHAR(32) NOT NULL,
	DateDeleted DATETIME,
	ExportID VARCHAR(32) NOT NULL
	);

CREATE TABLE ProjectCoC (
	ProjectCoCID VARCHAR(32) PRIMARY KEY NOT NULL,
	ProjectID VARCHAR(32) NOT NULL,
	CoCCode VARCHAR(6) NOT NULL,
	Geocode VARCHAR(6) NOT NULL,
	Address1 VARCHAR(100),
	Address2 VARCHAR(100),
	City VARCHAR(50),
	State VARCHAR(2),
	ZIP VARCHAR(5),
	GeographyType INT,
	DateCreated DATETIME NOT NULL,
	DateUpdated DATETIME NOT NULL,
	UserID VARCHAR(32) NOT NULL,
	DateDeleted DATETIME,
	ExportID VARCHAR(32) NOT NULL
	);

CREATE TABLE Services (
	ServicesID VARCHAR(32) PRIMARY KEY NOT NULL,
	EnrollmentID VARCHAR(32) NOT NULL,
	PersonalID VARCHAR(32) NOT NULL,
	DateProvided DATE NOT NULL,
	RecordType INT NOT NULL,
	TypeProvided INT NOT NULL,
	OtherTypeProvided VARCHAR(50),
	MovingOnOtherType VARCHAR(50),
	SubTypeProvided INT,
	FAAmount MONEY,
	FAStartDate DATE,
	FAEndDate DATE,
	ReferralOutcome INT,
	DateCreated DATETIME NOT NULL,
	DateUpdated DATETIME NOT NULL,
	UserID VARCHAR(32) NOT NULL,
	DateDeleted DATETIME,
	ExportID VARCHAR(32) NOT NULL
	);

CREATE TABLE "User" (
	UserID VARCHAR(32) PRIMARY KEY NOT NULL,
	UserFirstName VARCHAR(50),
	UserLastName VARCHAR(50),
	UserPhone VARCHAR(10),
	UserExtension VARCHAR(5),
	UserEmail VARCHAR(320),
	DateCreated DATETIME NOT NULL,
	DateUpdated DATETIME NOT NULL,
	DateDeleted DATETIME,
	ExportID VARCHAR(32) NOT NULL
	);

